%h3 Alexa Top 100 Sites

%table#alexa-top-sites.table.table-striped.table-hover.table-condensed
  %thead
    %tr
      %th Rank
      %th Site
      %th Page Views / Million
      %th Page Views / User
      %th.actions
  %tbody
    = render partial: 'top_site', collection: @top_sites

#alexa-top-site-modal.modal.fade{ role: 'dialog' }
  .modal-dialog{ role: 'document' }
    .modal-content
